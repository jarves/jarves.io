<!DOCTYPE html>
<html>
<head>
    <title>{{ pageResponse.getTitle() }}</title>
    <base href="{{ pageResponse.getBaseHref() }}" >
    <meta name="language" content="{{ pageResponse.getLanguage() }}" >
    <meta http-equiv="content-type" content="text/html; charset=utf-8" >

    <meta content='width=device-width' name='viewport'>
    <meta name="viewport" content="initial-scale=1.0" >


    <link rel="shortcut icon" href="{{ pageResponse.getFaviconPath() }}" type="image/x-icon" >
    <link rel="stylesheet" type="text/css" href="bundles/jarves/css/defaults.css" >

    {{ additionalHeaderTags|raw }}

    {{ cssTags|raw }}
    {{ jsTags|raw }}
    {{ assetTags|raw }}
</head>
<body>
{{ body|raw }}
{{ jsTagsBottom|raw }}
</body>
</html>